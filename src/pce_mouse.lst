   65                     
   66                             .macro spr_x
   67                              ldy   #2
   68                              .if (\# = 2)
   69                               lda   LOW_BYTE \1
   70                               clc
   71                               adc   LOW_BYTE \2
   72                               sta   [_si],Y
   73                               lda   HIGH_BYTE \1
   74                               adc   HIGH_BYTE \2
   75                              .else
   76                               lda   LOW_BYTE \1
   77                               sta   [_si],Y
   78                               lda   HIGH_BYTE \1
   79                              .endif
   80                              iny
   81                              sta   [_si],Y
   82                             .endm

   85                     ; spr_y(#y [,offset])
   86                     ; ----
   87                     ; y, the new y coordinate
   88                     
   89                             .macro spr_y
   90                              .if (\# = 2)
   91                               lda   LOW_BYTE \1
   92                               clc
   93                               adc   LOW_BYTE \2
   94                               sta   [_si]
   95                               lda   HIGH_BYTE \1
   96                               adc   HIGH_BYTE \2
   97                              .else
   98                               lda   LOW_BYTE \1
   99                               sta   [_si]
  100                               lda   HIGH_BYTE \1
  101                              .endif
  102                              ldy   #1
  103                              sta   [_si],Y
  104                             .endm
